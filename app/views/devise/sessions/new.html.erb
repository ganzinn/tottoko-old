<%= render "shared/header" %>
<div class="container">
<h5 class="screen-name">ログイン</h5>
<div class="contents">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="row">
  <div class="input-field col s12">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "validate" %>
    <%= f.label :email, 'email' %>
  </div>
</div>

<div class="row">
  <div class="input-field col s12">
    <%= f.password_field :password, autocomplete: "current-password", class: "validate" %>
    <%= f.label :password, 'パスワード' %>
  </div>
</div>  

<div class="button_wrapper">
  <%= button_tag '', class: 'btn-large waves-effect waves-light', type: 'button', onclick: 'submit();' do %>
    ログイン
  <% end %>
</div>

<% end %>

<%# <%= render "devise/shared/links" %>

</div>
</div>